{
  "name": "@volleytravel/vue-img-lazy",
  "version": "1.0.0",
  "repository": "https://github.com/volley/vue-img-lazy",
  "private": false,
  "scripts": {
    "build": "vue-cli-service build --target lib --name imgLazy src/ImgLazy.js",
    "lint": "vue-cli-service lint",
    "prepare": "yarn build",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "vanilla-lazyload": "^10.20.1",
    "vue": "^2.6.8"
  },
  "license": "MIT",
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-plugin-unit-jest": "^3.5.3",
    "@vue/cli-service": "^3.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "bdd-lazy-var": "^2.5.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "main": "dist/imgLazy.common.js",
  "files": [
    "dist/*",
    "src/*",
    ",*.json",
    "*.js"
  ]
}
